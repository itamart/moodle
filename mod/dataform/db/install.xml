        <SENTENCE TEXT="(module, action, mtable, field) VALUES ('dataform', 'views add', 'dataform_views', 'name')" />
        <SENTENCE TEXT="(module, action, mtable, field) VALUES ('dataform', 'views update', 'dataform_views', 'name')" />
        <SENTENCE TEXT="(module, action, mtable, field) VALUES ('dataform', 'filters add', 'dataform_filters', 'name')" />
        <SENTENCE TEXT="(module, action, mtable, field) VALUES ('dataform', 'filters update', 'dataform_filters', 'name')" />
      </SENTENCES>
    </STATEMENT>
  </STATEMENTS>
</XMLDB>